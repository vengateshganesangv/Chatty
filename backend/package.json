{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "build": "rimraf ./build && ttsc",
    "watch": "nodemon",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier --check src/**/*.{ts,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,json}",
    "window_test": "set NODE_ENV=development&& jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "linux_test": "export NODE_ENV=development&& jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "linux_start:dev": "npm run build && export NODE_ENV=development && node build/src/app.js | bunyan",
    "window_start:dev": "npm run build && set NODE_ENV=development && node build/src/app.js | bunyan",
    "linux_watch:dev": "export NODE_ENV=development&& npm run watch | bunyan",
    "window_watch:dev": "set NODE_ENV=development&& npm run watch | bunyan",
    "start:prod": "npm run build && set NODE_ENV=production && pm2 start build/src/app.js -i max --attach --watch | bunyan"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.0.3",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/nodemailer": "^6.4.6",
    "@types/swagger-stats": "^0.95.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^7.2.0",
