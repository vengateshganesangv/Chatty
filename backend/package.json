    "cloudinary": "^1.32.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.6.1",
    "redis": "^4.3.1",
    "socket.io": "^4.5.2",
    "swagger-stats": "^0.99.2"
  }
